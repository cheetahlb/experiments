test:
	${NPF_PATH}/npf-compare.py "local+cheetah,LB_MODE=awrr,report,REPORTPERIOD=10:Cheetah - AWRR" "local+cheetah,LB_MODE=pow2,report,REPORTPERIOD=10:Cheetah - Pow2" "local+cheetah,LB_MODE=least,report,REPORTPERIOD=10:Cheetah - Least Loaded" "local+cheetah,LB_MODE=rr:Cheetah - RR" "local+hash,rss:Hash RSS" --testie ${TESTIE_PATH} --cluster ${NPF_CLUSTER} --variables VERBOSE=0 PRINT=0 WORKSPACE=${CHEETAH_PATH} --tags stateless --output --variables "GEN_CONCURRENT=1024" "NSERVER={16,32,64}" --config n_runs=${N_RUNS} --tags rate --variables "SERVER_RATE=100" --show-full --show-cmd --graph-filename ${EXPE_PATH}/imbalance-bimodal/imbalance_server_bimodal_cpu/.pdf --tags wait --variables "WAIT=4000" FSIZE=8 GEN_TIME=10 --output-columns x all --graph-size 5 3 --config "graph_type={LAT999:boxplot,LAT99:boxplot,CPUVARS:boxplot}" "var_grid+={CPUVARS}" "graph_show_fliers=0" "legend_loc=outer lower center" "legend_ncol=3" "var_lim+={result-CPUVARS:0-1000,result-LAT99:500-3000}" "graph_color={1,2,3,5,2}" "graph_background+={CPUVARS:14}" --tags server_rate bimodal --variables LST_MODE=cpu --result-path ${EXPE_PATH}/imbalance-bimodal/.newbimodal --use-last ${NPF_FLAGS}
